
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
  password String?
}

model Item {
  id Int @default(autoincrement())
  name String 
  ShopName String
  ItemCode String @unique
}